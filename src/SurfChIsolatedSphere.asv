% This script file calculates and plots the surface charge of a isolated
% sphere, induced by a single charge in the vicinity. The unit is q./(4 pi
% a2)...a is the radius of the sphere
clear
gamma=linspace(-180,180)*pi/180;    % angle between charge and observer

a=1;
x=[2,4,6,8];    % distance of sphere
Qbyq=5;        % relation between Q and q

%potential function

for(n=1:4)
    pot(n,:)=(1./(sqrt(a^2+x(n)^2-2*(a*x(n)*cos(gamma)))))-((1*a)./(x(n)*sqrt(a^2+(a^4/x(n)^2)-2*(a^2/(x(n))*cos(gamma)))))+(Qbyq/a)+(a/x(n));
end

figure
plot(gamma,pot(1,:),gamma,pot(2,:),gamma,pot(3,:),gamma,pot(4,:))
xlabel('Angle/rad')
ylabel('potential at shere surface')
title('Potential on an isolated, conducting sphere; Q/q=5')
legend('x/a=2','x/a=4','x/a=6','x/a=8');

% surface charge


for(n=1:4)
    psi(n,:)=((1-(x(n)/a)*cos(gamma))./(1+(x(n).^2/a.^2)-2*(x(n)/a)*cos(gamma) ).^1.5)-((a/x(n))*(1-(a/x(n))*cos(gamma))./(1+(a.^2/x(n).^2)-2*(a/x(n))*cos(gamma)).^1.5)+(Qbyq)+1/x(n);
end

figure
plot(gamma,psi(1,:),gamma,psi(2,:),gamma,psi(3,:),gamma,psi(4,:))
xlabel('Angle/rad')
ylabel('Surface Charge/q/4 pi a.^2')
title('Surface charge on an isolated, conducting sphere; Q/q=5')
legend('x/a=2','x/a=4','x/a=6','x/a=8');


