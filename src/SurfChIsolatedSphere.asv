%--------------------------------------------------------------------------
%                           Using Debye Potential
%--------------------------------------------------------------------------

debye=10 %m ca sonnenwind

% compute potential

for(n=1:4)
    %pot(n,:)=(1./(sqrt(a^2+x(n)^2-2*(a*x(n)*cos(gamma)))).*exp(-(sqrt(2)*sqrt(a^2+x(n)^2-2*(a*x(n)*cos(gamma))))/(debye)))-((1*a)./(x(n)*sqrt(a^2+(a^4/x(n)^2)-2*(a*a^2/(x(n))*cos(gamma))))).*exp(-(sqrt(2)*sqrt(a^2+(a^4/x(n)^2)-2*(a^3/(x(n))*cos(gamma)))/(debye)))+((Qbyq/a)+(1/x(n))).*exp(-(sqrt(2)*0)/(debye));
    pot(n,:)=(1./(sqrt(a^2+x(n)^2-2*(a*x(n)*cos(gamma)))).*exp(-(sqrt(2)*sqrt(a^2+x(n)^2-2*(a*x(n)*cos(gamma))))/(debye)))-((1*a)./(x(n)*sqrt(a^2+(a^4/x(n)^2)-2*(a*a^2/(x(n))*cos(gamma))))).*exp(-(sqrt(2)*sqrt(a^2+(a^4/x(n)^2)-2*(a^3/(x(n))*cos(gamma)))/(debye)))+((Qbyq/a)+(1/x(n))).*exp(-(sqrt(2)*0)/(debye));

end

figure
plot(gamma,pot(1,:),gamma,pot(2,:),gamma,pot(3,:),gamma,pot(4,:))
xlabel('Angle/rad')
ylabel('potential at shere surface')
title('Potential on an isolated, conducting sphere; Q/q=5')
legend('x/a=2','x/a=4','x/a=6','x/a=8');

%--------------------------------------------------------------------------
% compute surface charge

for(n=1:4)
    A(n,:)=exp(-(sqrt(2*(1.+x(n)^2-2*x(n)*cos(gamma))))./(debye)).*(1.-x(n)*cos(gamma)).*((1)./(1+x(n).^2-2*x(n).*cos(gamma)).^(3/2)+(sqrt(2))./((1+x(n)^2-2*x(n)*cos(gamma))*debye));
    B(n,:)=1./x(n).*exp(-2*(sqrt(1.-1./x(n)*cos(gamma)))./(debye)).*(1.-1./x(n)*cos(gamma)).*((1)./(2-2*1./x(n).*cos(gamma)).^(3/2)+(1)./((1-1./x(n).*cos(gamma))*sqrt(2)*debye));
    C(n,:)=(Qbyq+1./x(n))*(sqrt(2)/debye)*exp(-(sqrt(2)*1/2)/(debye));
    psi(n,:)=(A(n,:)+B(n,:))+C(n,1);
end % for 
 
 figure
 plot(gamma,psi(1,:),gamma,psi(2,:),gamma,psi(3,:),gamma,psi(4,:))
xlabel('Angle/rad')
 ylabel('Surface Charge/q/4 pi a.^2')
 title('Surface charge on an isolated, conducting sphere; Q/q=5')
 legend('x/a=2','x/a=4','x/a=6','x/a=8');